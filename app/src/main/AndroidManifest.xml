<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tecstudyap.rasgullavideocallapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icoimage512"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icoimage512"
        android:supportsRtl="true"
        android:theme="@style/Theme.RasgullaVideoCallApp"
        android:usesCleartextTraffic="true"
        android:name=".ApplicationClass"
        >

        <activity
            android:name=".activities.OTPactivity"
            android:exported="false"
            android:windowSoftInputMode="stateVisible"/>
        <activity
            android:name=".activities.PhoneNumberActivity"
            android:exported="false"
            android:windowSoftInputMode="stateVisible"/>
        <activity
            android:name=".activities.TermsAndConditionsActivity"
            android:exported="false" />
        <activity
            android:name=".activities.RewardActivity"
            android:exported="false" />
        <activity
            android:name=".activities.CallActivity"
            android:exported="false" />
        <activity
            android:name=".activities.ConnectingActivity"
            android:exported="false" />
        <activity
            android:name=".activities.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".activities.WelcomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.MainActivity"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" /> <!-- Sample AdMob app ID: ca-app-pub-3940256099942544~3347511713 -->
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713" />
    </application>
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.RECORD_AUDIO" />

</manifest>